pub fn manifest() {
    let version = Version::new(0, 1, 0);

    Ok(Manifest {
        name: "my-project",
        version: version,
        authors: [
            Author::from_str("Alice"),
            Author::new("Bob"),
            Author::with_mail("Charlie", "example@example.com"),
        ],
        templates: #{}
    })
}
