use ::unvoid::{
    author::Author,
    manifest::Manifest,
    version::Version,
};

pub fn manifest() {
    let version = Version::parse_simple("0.1.0").unwrap();

    Manifest {
        name: "my-project",
        version: version,
        authors: [
            Author::new("Alice"),
            Author::new("Bob"),
            Author::new("Charlie").with_email("example@example.com"),
        ],
        templates: #{}
    }
}
